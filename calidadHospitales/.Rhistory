mediacontaminante<-function(directorio,contaminante,id){
x<-obtenerdirectorio(directorio,id[1])
for (i in id) {
x<-rbind(x,obtenerdirectorio(directorio,i))
}
x<-x[[contaminante]]
x[!is.na(x)]
mean(x)
}
